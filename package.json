{
    "name": "script-programming-language",
    "displayName": "Script Programming Language",
    "description": "Syntax Highlighting for Script",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "script",
            "aliases": ["Script", "sc"],
            "extensions": [".script"],
            "configuration": "./src/lang-config.json"
        }],
        "grammars": [{
            "language": "script",
            "scopeName": "source.script",
            "path": "./src/syntax.json"
        }]
    }
}